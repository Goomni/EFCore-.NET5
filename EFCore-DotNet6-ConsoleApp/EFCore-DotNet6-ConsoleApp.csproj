      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\EFCore-DotNet6-Data\EFCore-DotNet6-Data.csproj" />
    <ProjectReference Include="..\EFCore-DotNet6-Domain\EFCore-DotNet6-Domain.csproj" />
  </ItemGroup>

</Project>
